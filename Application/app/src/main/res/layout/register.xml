<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Witaj w Poznawajka Mobile. Zarejestruj się jako nowy użytkownik i zacznij korzystać z najlepszej platformy randkowej"
        android:textSize="17dp"
        android:id="@+id/title" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/TextViews"
        android:layout_below="@+id/title">

       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Imie"
            android:id="@+id/textView2" />

       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Nazwisko"
            android:id="@+id/textView3" />

       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Płeć"
            android:id="@+id/textView4" />
       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Wiek"
            android:id="@+id/textView5" />
       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Stan cywilny"
            android:id="@+id/textView6" />
       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Preferencje"
            android:id="@+id/textView7" />
       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Osobowość"
            android:id="@+id/textView8" />
       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="20dp"
            android:textSize="25dp"
            android:textStyle="bold"
            android:text="Email"
            android:id="@+id/textView9" />


    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout"
        android:layout_below="@+id/title"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:paddingTop="17dp">

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Podaj imie"
            android:textSize="17dp"
            android:paddingTop="17dp"
            android:ems="10"
            android:id="@+id/editText2"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Podaj nazwisko"
            android:textSize="17dp"
            android:paddingTop="17dp"
            android:ems="10"
            android:id="@+id/editText3"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Podaj płeć"
            android:textSize="17dp"
            android:paddingTop="17dp"
            android:ems="10"
            android:id="@+id/editText4"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Podaj Imie"
            android:paddingTop="17dp"
            android:textSize="17dp"
            android:ems="10"
            android:id="@+id/editText5"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Podaj stan cywilny"
            android:paddingTop="17dp"
            android:textSize="17dp"
            android:ems="10"
            android:id="@+id/editText6"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Wybierz preferencje"
            android:textSize="17dp"
            android:paddingTop="17dp"
            android:ems="10"
            android:id="@+id/editText7"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Wybierz cechy"
            android:textSize="17dp"
            android:paddingTop="17dp"
            android:ems="10"
            android:id="@+id/editText8"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Podaj email"
            android:textSize="17dp"
            android:paddingTop="17dp"
            android:ems="10"
            android:id="@+id/editText9"
            android:layout_below="@+id/title"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </LinearLayout>


</RelativeLayout>
